{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/v0.34.0/schema/markdownlint-config-schema.json",
  "default": true,
  "MD003": {
    "style": "atx_closed"
  },
  "single-h1": false,
  "no-hard-tabs": false,
  "whitespace": false,
  "MD007": {
    "indent": 4
  },
  "line-length": {
    "line_length": 150
  },
  "heading-style": {
    "style": "atx"
  },
  "ul-indent": {
    "indent": 2
  },
  "no-inline-html": false
}
